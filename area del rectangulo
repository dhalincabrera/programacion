#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int main (){

	float base, altura, area;
	cout<<"calculo del area del rectangulo\n";
	cout<<"ingrese la altura:";
	cin>> altura;
	cout<<"ingrese la base:";
	cin>> base;
	area =(base*altura);
	cout<<"el area del rectangulo es:";
	cout<<area;
	getch();
	
}
